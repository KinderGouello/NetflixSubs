{
	"manifest_version": 2,
	"name": "Netflix Subs",
	"description": "Add subtitles to a Netflix video",
	"version": "0.1.0",
	"permissions": [
		"http://*/*",
		"https://*/*",
        "downloads",
        "cookies"
	],
    "browser_action": {
    	"default_icon" : "img/icon128.png",
    	"default_popup": "popup.html"
    },
    "icons": {
    	"128" : "img/icon128.png"
    }
}